<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping
    schema="aa"
>

<class 
    name="de.escidoc.core.aa.business.persistence.UserPreference" 
    table="user_preference"
    lazy="false">
    <meta attribute="generated-class">
				de.escidoc.core.aa.business.persistence.UserPreferenceBase </meta>
				<meta attribute
			="class-description" inherit="false">
       @hibernate.class
        table="user_preference"
       @hibernate.mapping
        schema="aa"
    </meta>

    <id
        name="id"
        type="java.lang.String"
        column="id"
    >
    
        <meta attribute="generated-class">
				de.escidoc.core.aa.business.persistence.UserPreferenceBase</meta>
				<meta attribute
			="field-description">
           @hibernate.id
            generator-class="uuid"
            type="java.lang.String"
            column="id"

        </meta>
        <generator class="uuid" />
    </id>
    <!-- composite-id name="id" class="UserPreferencePK">
    	<meta attribute="generated-class">
				de.escidoc.core.aa.business.persistence.UserPreferenceBase</meta>
				<meta attribute
			="field-description">
           @hibernate.id
            column="id"

        </meta>
      <key-property name="user_id" type="string" column="user_id"/>
      <key-property name="name" type="string" column="name"/>
      <!- key-many-to-one name="address" class="Address" column="addr_id"/ ->
    </composite-id --> 
    
    <property
        name="name"
        type="java.lang.String"
        column="name"
        length="255"
    >
        <meta attribute="generated-class">
				de.escidoc.core.aa.business.persistence.UserPreferenceBase </meta>
				<meta attribute
			="field-description">
           @hibernate.property
            column="name"
            length="255"
        </meta>    
    </property>
    
    <property
        name="value"
        type="java.lang.String"
        column="value"
        length="255"
    >
        <meta attribute="generated-class">
				de.escidoc.core.aa.business.persistence.UserPreferenceBase </meta>
				<meta attribute
			="field-description">
           @hibernate.property
            column="value"
            length="255"
        </meta>    
    </property>
    
    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to UserAccount -->
    <many-to-one
        name="userAccountByUserId"
        class="de.escidoc.core.aa.business.persistence.UserAccount"
    >
        <meta attribute="generated-class">
				de.escidoc.core.aa.business.persistence.UserPreferenceBase </meta>
				<meta attribute
			="field-description">
           @hibernate.many-to-one
           @hibernate.column name="user_id"
        </meta>
        <column name="user_id" />
    </many-to-one>
    
</class>
</hibernate-mapping>
