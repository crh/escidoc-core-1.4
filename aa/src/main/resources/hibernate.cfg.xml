<!DOCTYPE hibernate-configuration PUBLIC
"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
	<session-factory>
		<mapping resource="META-INF/hibernate/Action.hbm.xml" />
		<mapping resource="META-INF/hibernate/EscidocPolicy.hbm.xml" />
		<mapping resource="META-INF/hibernate/EscidocRole.hbm.xml" />
		<mapping resource="META-INF/hibernate/RoleGrant.hbm.xml" />
        <mapping resource="META-INF/hibernate//ScopeDef.hbm.xml" />
		<mapping resource="META-INF/hibernate/UnsecuredActionList.hbm.xml" />
		<mapping resource="META-INF/hibernate/UserAccount.hbm.xml" />
		<mapping resource="META-INF/hibernate/UserAttribute.hbm.xml" />
		<mapping resource="META-INF/hibernate/UserGroup.hbm.xml" />
		<mapping resource="META-INF/hibernate/UserGroupMember.hbm.xml" />
		<mapping resource="META-INF/hibernate/UserLoginData.hbm.xml" />
		<mapping resource="META-INF/hibernate/UserPreference.hbm.xml" />
	</session-factory>
</hibernate-configuration>
