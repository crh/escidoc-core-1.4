<${RDF_NAMESPACE_PREFIX}:RDF xmlns:${RDF_NAMESPACE_PREFIX}="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"	xmlns:${CONTAINER_NAMESPACE_PREFIX}="${CONTAINER_NAMESPACE}" xmlns:xlink="http://www.w3.org/1999/xlink/" xmlns:${RELATIONS_NAMESPACE_PREFIX}="${RELATIONS_NAMESPACE}">
<${RDF_NAMESPACE_PREFIX}:Description ${RDF_NAMESPACE_PREFIX}:about="info:fedora/${OBJID}">
<${CONTAINER_NAMESPACE_PREFIX}:name>${NAME}</${CONTAINER_NAMESPACE_PREFIX}:name>
<${CONTAINER_NAMESPACE_PREFIX}:description>${DESCRIPTION}</${CONTAINER_NAMESPACE_PREFIX}:description>
<${RELATIONS_NAMESPACE_PREFIX}:title>${TITLE}</${RELATIONS_NAMESPACE_PREFIX}:title>
<${RELATIONS_NAMESPACE_PREFIX}:objectType>${OBJECT_TYPE}</${RELATIONS_NAMESPACE_PREFIX}:objectType>
<${CONTAINER_NAMESPACE_PREFIX}:creator ${RDF_NAMESPACE_PREFIX}:resource="info:fedora/${CREATOR_ID}" />
<${CONTAINER_NAMESPACE_PREFIX}:creator-title>${CREATOR_TITLE}</${CONTAINER_NAMESPACE_PREFIX}:creator-title>
<${CONTAINER_NAMESPACE_PREFIX}:context ${RDF_NAMESPACE_PREFIX}:resource="info:fedora/${CONTEXT_ID}" />
<${CONTAINER_NAMESPACE_PREFIX}:context-title>${CONTEXT_TITLE}</${CONTAINER_NAMESPACE_PREFIX}:context-title>
<${CONTAINER_NAMESPACE_PREFIX}:content-type ${RDF_NAMESPACE_PREFIX}:resource="info:fedora/${CONTENT_TYPE_ID}" />
<${CONTAINER_NAMESPACE_PREFIX}:content-type-title>${CONTENT_TYPE_TITLE}</${CONTAINER_NAMESPACE_PREFIX}:content-type-title>
<${CONTAINER_NAMESPACE_PREFIX}:status>${STATUS}</${CONTAINER_NAMESPACE_PREFIX}:status>
<${RELATIONS_NAMESPACE_PREFIX}:hasAdminDescriptor ${RDF_NAMESPACE_PREFIX}:resource="info:fedora/${ADMIN_DESCRIPTOR_ID}"/>
<${CONTAINER_NAMESPACE_PREFIX}:latest-version.number>${LATEST_VERSION_NUMBER}</${CONTAINER_NAMESPACE_PREFIX}:latest-version.number>
<${CONTAINER_NAMESPACE_PREFIX}:latest-version.date>${LATEST_VERSION_DATE}</${CONTAINER_NAMESPACE_PREFIX}:latest-version.date>
<${CONTAINER_NAMESPACE_PREFIX}:latest-version.status>${LATEST_VERSION_STATUS}</${CONTAINER_NAMESPACE_PREFIX}:latest-version.status>
<${CONTAINER_NAMESPACE_PREFIX}:latest-version.valid-status>${LATEST_VERSION_VALID_STATUS}</${CONTAINER_NAMESPACE_PREFIX}:latest-version.valid-status>
<${CONTAINER_NAMESPACE_PREFIX}:latest-version.user>${LATEST_VERSION_USER}</${CONTAINER_NAMESPACE_PREFIX}:latest-version.user>
${MEMBERS}
</${RDF_NAMESPACE_PREFIX}:Description>
</${RDF_NAMESPACE_PREFIX}:RDF>

