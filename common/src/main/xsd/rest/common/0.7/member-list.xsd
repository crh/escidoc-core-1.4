<xs:schema 
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:item="http://www.escidoc.de/schemas/item/0.7"
	xmlns:container="http://www.escidoc.de/schemas/container/0.7"
	xmlns:common-types="http://www.escidoc.de/schemas/commontypes/0.4"
	targetNamespace="http://www.escidoc.de/schemas/memberlist/0.7"
	elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:import namespace="http://www.w3.org/XML/1998/namespace"
		schemaLocation="http://www.escidoc.org/schemas/rest/common/0.3/xml.xsd" />
	<xs:import namespace="http://www.escidoc.de/schemas/item/0.7"
		schemaLocation="http://www.escidoc.org/schemas/rest/item/0.7/item.xsd" />
	<xs:import namespace="http://www.escidoc.de/schemas/container/0.7"
		schemaLocation="http://www.escidoc.org/schemas/rest/container/0.7/container.xsd" />
		<xs:import namespace="http://www.escidoc.de/schemas/commontypes/0.4" 
		schemaLocation="http://www.escidoc.org/schemas/rest/common/0.4/common-types.xsd"/>
	<xs:element name="member-list">
		<xs:complexType>
			<xs:sequence>
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:element ref="item:item" />
					<xs:element ref="container:container" />
				</xs:choice>
			</xs:sequence>
			<xs:attributeGroup ref="common-types:readOnlyESciDocResourceLinkAttributes" />
			<xs:attribute name="number-of-records" use="optional"/>
			<xs:attribute name="offset" use="optional"/>
			<xs:attribute name="limit" use="optional"/>
		</xs:complexType>
	</xs:element>
</xs:schema>



