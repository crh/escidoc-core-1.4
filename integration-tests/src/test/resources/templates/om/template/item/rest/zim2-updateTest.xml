<?xml version="1.0" encoding="UTF-8"?>
<escidocItem:item xmlns:prefix-dc="http://purl.org/dc/elements/1.1/"
	xmlns:prefix-dcterms="http://purl.org/dc/terms/"
	xmlns:eidt="http://escidoc.mpg.de/metadataprofile/schema/0.1/idtypes"
	xmlns:escidoc="http://escidoc.mpg.de/metadataprofile/schema/0.1/types"
	xmlns:escidocAccountUser="http://www.escidoc.de/schemas/useraccount/0.2"
	xmlns:escidocComponents="http://www.escidoc.de/schemas/components/0.9"
	xmlns:escidocItem="http://www.escidoc.de/schemas/item/0.10"
	xmlns:escidocItemList="http://www.escidoc.de/schemas/item-list"
	xmlns:escidocMetadataProfile="http://escidoc.mpg.de/metadataprofile/schema/0.1/"
	xmlns:escidocMetadataRecords="http://www.escidoc.de/schemas/metadatarecords/0.5"
	xmlns:escidocRelations="http://www.escidoc.de/schemas/relations/0.4"
	xmlns:escidocResources="http://www.escidoc.de/schemas/resources/0.2"
	xmlns:prefix-xlink="http://www.w3.org/1999/xlink"
	xmlns:prefix-xsi="http://www.w3.org/2001/XMLSchema-instance"
	prefix-xlink:title="Object title"
	prefix-xlink:type="simple" prefix-xlink:href="/ir/item/##ITEMID##"
	prefix-xsi:schemaLocation="http://www.escidoc.de/schemas/item item/0.2/item.xsd"
	last-modification-date="##LASTMODIFICATIONDATE##">
	<escidocItem:properties
		xmlns:prop="http://escidoc.de/core/01/properties/" 
		xmlns:srel="http://escidoc.de/core/01/structural-relations/"
		prefix-xlink:href="/ir/item/##ITEMID##/properties"
		prefix-xlink:title="Properties" prefix-xlink:type="simple">
		<prop:creation-date>
			##CREATIONDATE##
		</prop:creation-date>
		<srel:created-by
			prefix-xlink:href="/aa/user-account/escidoc:user42"
			prefix-xlink:type="simple" prefix-xlink:title="Skip Authorization" />
		<prop:public-status>pending</prop:public-status>
		<srel:context
			prefix-xlink:href="/ir/context/escidoc:persistent3"
			prefix-xlink:type="simple"
			prefix-xlink:title="Test Collection"/>
		<srel:content-model
			prefix-xlink:href="/cmm/content-model/escidoc:persistent4"
			prefix-xlink:type="simple" prefix-xlink:title="content-type" />

		<prop:lock-status>unlocked</prop:lock-status>

		<prop:version prefix-xlink:type="simple"
		 	xmlns:version="http://escidoc.de/core/01/properties/version/"
			prefix-xlink:title="current version"
			prefix-xlink:href="/ir/item/##ITEMID##:1">
			<version:number>1</version:number>
			<version:date>
				##LASTMODIFICATIONDATE##
			</version:date>
			<version:status>
				pending
			</version:status>
			
			<srel:modified-by prefix-xlink:type="simple"
				prefix-xlink:title="roland"
				prefix-xlink:href="/aa/user-account/escidoc:user42" />
			<version:comment>comment</version:comment>
		</prop:version>
		<prop:latest-version prefix-xlink:type="simple"
		 	xmlns:version="http://escidoc.de/core/01/properties/version/"
			prefix-xlink:title="latest version"
			prefix-xlink:href="/ir/item/##ITEMID##:1">
			<version:number>1</version:number>
			<version:date>
				##LASTMODIFICATIONDATE##
			</version:date>
		</prop:latest-version>
		<prop:content-model-specific>
			<prop:anything />
		</prop:content-model-specific>
	</escidocItem:properties>
	<escidocMetadataRecords:md-records>
		<escidocMetadataRecords:md-record prefix-xlink:type="simple"
			prefix-xlink:title="MD-record title" name="escidoc">
			<escidocMetadataProfile:publication type="book">
				<escidoc:creator role="author">
					<escidoc:person>
						<escidoc:complete-name>
							Hans Meier
						</escidoc:complete-name>
						<escidoc:family-name>Meier</escidoc:family-name>
						<escidoc:given-name>Hans</escidoc:given-name>
					</escidoc:person>
				</escidoc:creator>
				<prefix-dc:title xml:lang="en">The title updated ******</prefix-dc:title>
			</escidocMetadataProfile:publication>
		</escidocMetadataRecords:md-record>
	</escidocMetadataRecords:md-records>
	<escidocItem:resources prefix-xlink:type="simple"
		prefix-xlink:href="/ir/item/##ITEMID##/resources"
		prefix-xlink:title="Virtual Resources">
		<escidocItem:version-history prefix-xlink:type="simple"
			prefix-xlink:title="Version History"
			prefix-xlink:href="/ir/item/##ITEMID##/resources/version-history" />
	</escidocItem:resources>
</escidocItem:item>
